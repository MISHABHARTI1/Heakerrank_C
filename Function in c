#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

     
int max_of_four(int a, int b, int c,int d)
    {
      int max;
      max= a>b?a:b;
      max= c>max?c:max;
      max = d>max?d:max;
      return max;
    } 

 int main()
 {
    int a,b,c,d;
     scanf("%d\n %d\n %d\n %d\n", &a, &b, &c, &d);
     int x=max_of_four(a,b,c,d);
     printf("%d", x);
    
     return 0;
}
